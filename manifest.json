{
	"manifest_version": 3,
	"name": "LeetCode",
	"version": "1.0",
	"description": "My LeetCode Customization",
	"permissions": ["scripting"],
	"host_permissions": ["https://leetcode.com/*"],
	"content_scripts": [
		{
			"matches": ["https://leetcode.com/*"],
			"js": ["content.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
